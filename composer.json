{
    "name": "insanitymeetshh/slim-skeleton",
    "description": "Slim 3 Skeleton Fork - InsanityMeetsHH",
    "version": "4.0.1",
    "keywords": ["microframework", "rest", "router", "psr7", "doctrine", "twig", "localisation", "acl", "login", "csrf"],
    "homepage": "https://github.com/InsanityMeetsHH/Slim-Skeleton",
    "license": "MIT",
    "authors": [
        {
            "name": "InsanityMeetsHH",
            "email": "software@insanitymeetshh.net",
            "homepage": "http://www.insanitymeetshh.net/"
        },
        {
            "name": "Josh Lockhart",
            "email": "info@joshlockhart.com",
            "homepage": "http://www.joshlockhart.com/"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "doctrine/orm": "^2.6",
        "geggleto/psr7-acl": "^1.3",
        "monolog/monolog": "^1.17",
        "slim/csrf": "^0.8.2",
        "slim/slim": "^3.1",
        "slim/twig-view": "^2.3"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4.8 < 6.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "process-timeout" : 0
    },
    "scripts": {
        "start": "php -S localhost:8080 -t public public/index.php",
        "test": "phpunit"
    }

}
